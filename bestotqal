\title{Measurements of Branching Ratio of $J/\psi \to 3\gamma$ and $J/\psi \to \gamma \eta_c$ at BESIII}
% for the authors
\author[1]{Tianjiao Zhang \& Dianyu Liu }
\author[1]{Jifeng Hu}
